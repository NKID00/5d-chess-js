{
  "name": "5d-chess.js",
  "version": "0.1.0",
  "description": "Open source implementation of '5D Chess With Multiverse Time Travel' in the style of Chess.js library.",
  "main": "./src/index.js",
  "files": [
    "./dist/*.js"
  ],
  "scripts": {
    "cli": "nodemon ./src/cli.js",
    "test": "jest",
    "profile": "clinic flame -- node ./src/bench.js",
    "prepublishOnly": "npm run-script test && npm run-script build && npm run-script minify",
    "build": "browserify  ./src/index.js -t aliasify -o ./dist/5d-chess.js",
    "minify": "browserify  ./src/index.js -p tinyify -t aliasify -o ./dist/5d-chess.min.js"
  },
  "aliasify": {
    "aliases": {
      "@local": "./src"
    }
  },
  "_moduleAliases": {
    "@local": "./src"
  },
  "jest": {
    "moduleNameMapper": {
      "^@local(.*)$": "<rootDir>/src/$1"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/alexbay218/5d-chess-js.git"
  },
  "author": "Shaun Wu",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://gitlab.com/alexbay218/5d-chess-js/issues"
  },
  "keywords": [
    "chess",
    "5d",
    "5d-chess",
    "javascript",
    "commonjs",
    "npm",
    "node",
    "browser"
  ],
  "homepage": "https://gitlab.com/alexbay218/5d-chess-js#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "deep-copy": "^1.4.2",
    "deep-equal": "^2.0.3",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "browserify": "^16.5.2",
    "aliasify": "^2.1.0",
    "clinic": "^7.0.0",
    "jest": "^26.4.2",
    "tinyify": "^3.0.0",
    "uglify-js": "^3.10.4"
  }
}
